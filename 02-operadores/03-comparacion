let a = 10;

//Relacionales 
console.log(a <5);
console.log(a >=5);
console.log(a <5);
console.log(a <= 5);


//Igualdad
console.log(a == 10);
console.log(a != 10);
console.log(a == '10');// == valores iguales, no pregunta por tipo igual 
console.log(a === '10');// == valores iguales y tipo igual 
console.log(a !== '10');// distintos valores iguales y tipo igual 